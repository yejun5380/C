# 6주차 c언어
  -6주차 
.
.
~~~c
#include <stdio.h>

int main(void)
{
    char op;
    int x,t,result;
    printf("수식을 입력하시오(예:2+5)>>");
    scanf("%d %c %d", &x, &op, &y);

switch (op)
    {
        case '+':
            result = x+y;
            break;
        case '-':
            result = x-y;
            break;
        case '*':
            result = x*y;
            break;
        case '/':
            result = x/y;
            break;
        case '%':
            result = x%y;
            break;
        default:
            printf("지원되지 않는 연산자입니다.\n");
            break;
    }
    printf("%d %c %d = %d\n",x ,op ,y ,result);
    return 0;

}
~~~


~~~c
#include <stdio.h>

int main(void)
{
    char op;
    int x, y, result;

    printf("수식을 입력하시오(예:2+5)>>");
    scanf("%d %c %d", &x, &op, &y);

    if (op=='+')
        result = x+y;
    else if(op=='-')
        result = x-y;
    else if(op=='*')
        result = x*y;
    else if(op=='/')
        result = x/y;
    else if(op=='%')
        result = x%y;
    else
            printf("지원되지 않는 연산자입니다.");

    printf("%d %c %d=%d\n", x, op, y, result);
    return 0;    
}
~~~


~~~c
#include <stdio.h>

int main(void)
{
    int i=1;

    while(i<10)
    {
        printf("%d * %d = %d\n", 2, i, 2*i);
        i++;
        if(i>9)
        {
            break;
        }
    }
}
~~~



